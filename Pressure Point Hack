const handlers = {
    'LaunchRequest': function () {
      if(supportsDisplay.call(this)||isSimulator.call(this)) {
          console.log("has display:"+ supportsDisplay.call(this));
          console.log("is simulator:"+isSimulator.call(this));
          var content = {
             "templateToken" : "factBodyTemplate",
             "listItems": "listItems",
             "askOrTell" : ":ask",
             "sessionAttributes": this.attributes
          };
          renderTemplate.call(this, content);
          } else {
         let card = "You can ask for a pressure point hack based on your need\n Such as Stress, Dizziness, Sinus Pain, Eye Tiredness,\n Back pain, Digestion, Appetite, Constipation,\n Neck Pain, Breathing Problems, Nausea, Stomach Pain,\n Headache, Sciatica, Arthritis, Water Waste, and Anxiety.\n You can also request a pressure point based on the\n a part of you body such as leg, face, elbow, foot, and abdominal. \nWhich would you like?";
         this.response.cardRenderer(SKILL_CARD_TITLE, strip.stripTags(card));
         this.response.speak('You can ask for a pressure point hack based on your need\n Such as Stress, Dizziness, and Sciatica. Or you can say random or based on the a part of the body, enclosed is a complete list in the Skill Card... \nHow can I help?')
                      .listen("Which would you like?");
         this.emit(':responseReady');
        }
    },
    'PressurePointHack': function () {
        let points = randomArrayElement(getVoice(''));
        this.attributes['pressure'] = points.tip;
        this.attributes['pressure'] = points.name;
        this.attributes['pressure'] = points.text;
        this.attributes['pressure'] = points.smallImage;
        this.attributes['pressure'] = points.largeImage;

        let speechOutput =  "Here is a random pressure point" + points.tip + BREAK + STOP_MESSAGE;
        let respeech =  "Would you like to try again?";
        let card = points.text +"\n" + points.text;
        let imageObj = {smallImageUrl: points.smallImage, largeImageUrl: points.largeImage};
        
        this.response.cardRenderer(SKILL_CARD_TITLE, strip.stripTags(card), imageObj);
        this.response.speak(speechOutput);
                     //.listen(respeech);
        this.emit(':responseReady');
    },
    'pressureIntent': function(){
        var pointName;
        
        if (this.event.request.intent.slots.pointSlot.value)
            pointName = this.event.request.intent.slots.pointSlot.value.toLowerCase();
         
                if (pointName == 'hand')
                {
                    let points = randomArrayElement(getBylocation('Hand'));
                    this.attributes['pressure'] = points.tip;
                    this.attributes['pressure'] = points.name;
                    this.attributes['pressure'] = points.text;
                    this.attributes['pressure'] = points.smallImage;
                    this.attributes['pressure'] = points.largeImage;

                    let speechOutput =  points.tip;
                    let card = points.name +"\n" + points.text;
                    let imageObj = {smallImageUrl: points.smallImage, largeImageUrl: points.largeImage};
        
                    this.response.cardRenderer(SKILL_CARD_TITLE, strip.stripTags(card), imageObj);
        
                    this.response.speak(speechOutput);
                    this.emit(':responseReady');
                }
                else if (pointName == 'foot')
                {
                    let points = randomArrayElement(getBylocation('Foot'));
                    this.attributes['pressure'] = points.tip;
                    this.attributes['pressure'] = points.name;
                    this.attributes['pressure'] = points.text;
                    this.attributes['pressure'] = points.smallImage;
                    this.attributes['pressure'] = points.largeImage;

                    let speechOutput =  points.tip;
                    let card = points.name +"\n" + points.text;
                    let imageObj = {smallImageUrl: points.smallImage, largeImageUrl: points.largeImage};
        
                    this.response.cardRenderer(SKILL_CARD_TITLE, strip.stripTags(card), imageObj);
        
                    this.response.speak(speechOutput);
                    this.emit(':responseReady');
                }
                else if (pointName == 'leg')
                {
                    let points = randomArrayElement(getBylocation('Leg'));
                    this.attributes['pressure'] = points.tip;
                    this.attributes['pressure'] = points.name;
                    this.attributes['pressure'] = points.text;
                    this.attributes['pressure'] = points.smallImage;
                    this.attributes['pressure'] = points.largeImage;

                    let speechOutput =  points.tip;
                    let card = points.name +"\n" + points.text;
                    let imageObj = {smallImageUrl: points.smallImage, largeImageUrl: points.largeImage};
        
                    this.response.cardRenderer(SKILL_CARD_TITLE, strip.stripTags(card), imageObj);
        
                    this.response.speak(speechOutput);
                    this.emit(':responseReady');
                }
                else if (pointName == 'abdomen')
                {
                    let points = randomArrayElement(getBylocation('Abdomen'));
                    this.attributes['pressure'] = points.tip;
                    this.attributes['pressure'] = points.name;
